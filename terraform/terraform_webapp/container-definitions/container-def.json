[{
    "name": "myapp",
    "image": "servian/techchallengeapp:latest",
    "cpu": 10,
    "memory": 256,
    "essential": true,
    "portMappings": [{
        "containerPort": 80
    }],
    "command": [
        "serve"
    ],
    "environment": [{
            "name": "VTT_DBUSER",
            "value": "postgres"
        },
        {
            "name": "VTT_DBPASSWORD",
            "value": "password123"
        },
        {
            "name": "VTT_DBNAME",
            "value": "app"
        },
        {
            "name": "VTT_DBPORT",
            "value": "5432"
        },
        {
            "name": "VTT_DBHOST",
            "value": "production.cew4zrpigsgp.ap-southeast-2.rds.amazonaws.com"
        },
        {
            "name": "VTT_LISTENHOST",
            "value": "0.0.0.0"
        },
        {
            "name": "VTT_LISTENPORT",
            "value": "80"
        }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "/ecs/frontend-container",
            "awslogs-region": "ap-southeast-2"
        }
    }
}]